{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "outDir": "./build"
      
    },
    "include": ["*.ts", "./src/*"],
    "exclude": ["node_modules"]
  }
  